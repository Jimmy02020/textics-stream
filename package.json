{
  "name": "textics-stream",
  "version": "0.0.1",
  "description": "Node Text Statistics For Streams",
  "main": "lib/tStream.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha  --compilers js:babel-core/register --recursive test",
    "test:gen": "mocha   --compilers js:babel-core/register test/general.test.js -w",
    "test:oneline": "mocha   --compilers js:babel-core/register test/one.line.ending.test.js -w",
    "test:noline": "mocha   --compilers js:babel-core/register test/no.line.ending.test.js -w",
    "test:w": "mocha   --compilers js:babel-core/register --recursive test -w",
    "test:cov": "istanbul cover node_modules/mocha/bin/_mocha --  --compilers js:babel-core/register --recursive test",
    "check:cov": "istanbul check-coverage --root coverage --statement 100 --branch 100 --function 100 --line 100",
    "report:cov": "cat ./coverage/lcov.info | codecov",
    "prebuild": "rimraf lib",
    "build": "webpack -p",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimmy02020/textics-stream.git"
  },
  "keywords": [
    "backend",
    "transform",
    "text",
    "string",
    "statistics",
    "stat",
    "stream"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": {
    "name": "Jalal Maskoun",
    "email": "jimmy002020@gmail.com"
  },
  "license": "'MIT'",
  "bugs": {
    "url": "https://github.com/Jimmy02020/textics-stream/issues"
  },
  "homepage": "https://github.com/Jimmy02020/textics-stream#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "benchmark": "2.1.4",
    "chai": "4.1.1",
    "codecov.io": "0.1.6",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "webpack": "3.5.5"
  },
  "dependencies": {
    "textics": "0.1.1"
  }
}
